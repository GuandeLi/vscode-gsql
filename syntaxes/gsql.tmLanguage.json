{
  "scopeName": "source.gsql",
  "patterns": [
    { "include": "#m-comment" },
    { "include": "#keyword" }
  ],
  "repository": {
    "m-comment": {
      "begin": "/\\*",
      "end": "\\*/",
      "name": "comment.block.gsql"
    },
    "keyword": {
      "match": "(?i)\\b(ABORT|ACCUM|AND|ANY|API|AS|ASC|AVG|BAG|BATCH|BETWEEN|BOOL|BOTH|BREAK|BY|CASE|CATCH|COALESCE|COMMIT|COMPRESS|CONTINUE|COUNT|CREATE|DATETIME|DATETIME_ADD|DATETIME_SUB|DELETE|DESC|DISTINCT|DISTRIBUTED|DO|DOUBLE|EDGE|ELSE|END|ESCAPE|EXCEPTION|FALSE|FILE|FILTER|FLOAT|FOR|FOREACH|FROM|GRAPH|GROUP|HAVING|IF|IN|INSERT|INT|INTERPRET|INTERSECT|INTERVAL|INTO|IS|ISEMPTY|JSONARRAY|JSONOBJECT|LASTHOP|LEADING|LIKE|LIMIT|LIST|LOAD_ACCUM|LOG|MAP|MATCH|MAX|MIN|MINUS|NOT|NOW|NULL|OFFSET|OR|ORDER|PATH|PER|PINNED|POST_ACCUM|POST-ACCUM|PRIMARY_ID|PRINT|QUERY|RAISE|RANGE|REPLACE|RETURN|RETURNS|RUN|SAMPLE|SELECT|SELECT_VERTEX|SET|SRC|STATIC|STRING|SUM|SYNTAX|TARGET|TAGS|TGT|THEN|TO|TO_CSV|TO_DATETIME|TRAILING|TRIM|TRUE|TRY|TUPLE|TYPEDEF|UINT|UNION|UPDATE|VALUES|VERTEX|WHEN|WHERE|WHILE|WITH|GSQL_INT_MAX|GSQL_INT_MIN|GSQL_UINT_MAX|RESET_COLLECTION_ACCUM)\\b",
      "name": "keyword.control.gsql"
    }
  }
}